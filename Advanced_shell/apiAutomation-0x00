#!/bin/bash

#File paths
DATA_FILE="data.json"
ERROR_FILE="errors.txt"

# API endpoint
URL="https://pokeapi.co/api/v2/pokemon/pikachu"

# Make the request
curl -s -f "$URL" -o "$DATA_FILE"

# Check if curl was successful
if [ $? -ne 0 ]; then
    echo "Failed to fetch data from $URL" >> "$ERROR_FILE"
fi
